let sum = 0; // sum - змінна,  в якій потім передамо загальну суму чисел
for (let i = 1; i <= 100; i++) { // for пройде по всім вичлам від 1 до 100, далі отрімає folse (на 101) та зупиниться
    if (i % 3 !== 0) { // це умова, за якою,  якщо після ділення на 3 не буде залишку,  то це число додавитиметься до суми sum
      sum += i;
    }
  }
  
  console.log(sum); 

